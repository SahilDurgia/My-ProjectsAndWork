.parent{
	background-color:red;
	width:1000px;
	height:500px;
	position:relative;
}

.child1{
	width:100px;
	height:100px;
	background-color:blue;
	top:0px;
}
.child2{
	width:100px;
	height:100px;
	background-color:yellow;
	bottom:0px;
	position:absolute;
	
}
.child3{
	width:100px;
	height:100px;
	background-color:purple;
	top:50%;
	left:50%;
	/* top:200px; */
	/* left:450px; */
	position:absolute;
	
}
.child4{
	width:100px;
	height:100px;
	background-color:pink;
	top:0px;
	right:0px;
	position:absolute;
}
.child5{
	width:100px;
	height:100px;
	background-color:green;
	bottom:0px;
	right:0px;
	position:absolute;
}
.fixed{
	width:100px;
	height:100px;
	background-color:aqua;
	position:sticky;
	top:0px;
}


